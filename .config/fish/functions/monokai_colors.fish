# Monokai Pro Light Colors
set fish_color_normal 222222 # default color - dark gray
set fish_color_command 594698 # commands - purple
set fish_color_quote D4572A # quoted text - yellow
set fish_color_redirection 887dc1 # IO redirections - purple
set fish_color_end 939293 # separators - medium gray
set fish_color_error 939293 --background=FF6188 # errors - white on pink/red
set fish_color_param 218871 # parameters - green
set fish_color_comment 939293 # comments - medium gray
set fish_color_match 222222 # matching parentheses - dark gray
set fish_color_search_match --background=939293 # search matches - light gray background
set fish_color_operator 887dc1 # operators - purple
set fish_color_escape 78DCE8 # escape characters - light blue
set fish_color_cwd 78DCE8 # current working directory - light blue

# Completion pager colors
set fish_pager_color_prefix 222222 # prefix - dark gray
set fish_pager_color_completion 939293 # completion text - medium gray
set fish_pager_color_description 939293 # descriptions - light gray
set fish_pager_color_progress 222222 # progress bar - dark gray
set fish_pager_color_secondary FFFFFF # secondary background - white
